{
  "name": "cep-crawler",
  "version": "1.0.0",
  "description": "Sistema ass√≠ncrono de processamento de CEPs com fila de mensagens",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/workers/cepWorker.ts",
    "start": "node dist/index.js",
    "start:worker": "node dist/workers/cepWorker.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "cep",
    "crawler",
    "queue",
    "async",
    "worker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.960.0",
    "axios": "^1.6.2",
