    "express-rate-limit": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.4.1",
    "mongodb": "^6.3.0",
    "p-limit": "^3.1.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "prom-client": "^15.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
